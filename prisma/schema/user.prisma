model User {
    id       String     @id @default(uuid())
    name     String     @db.VarChar(20)
    email    String     @unique
    role     UserRole   @default(CUSTOMER)
    password String
    status   UserStatus
}

enum UserRole {
    CUSTOMER
    PROVIDER
    ADMIN
}

enum UserStatus {
    SUSPEND
    ACTIVATE
}
