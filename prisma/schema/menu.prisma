model Menu {
    id         String   @id @default(uuid())
    providerId String
    name       String   @db.VarChar(50)
    category   String   @db.VarChar(50)
    cuisine    String   @db.VarChar(50)
    createdAt  DateTime @default(now())
    updatedAt  DateTime @default(now())
    orders     Order[]
    review     Review[]
}
